								img.back-home(src='./assets/back-home.png', height='15', alt='back home button')
						// SLEEPS: 1/2 ...31/40
						.numbers.col-6
							.container
								.row
									.sleeps.col-2 sleeps:
									.range-small.col-1
										a(href='https://www.brightonholidayhomes.co.uk/sleeps/range/1-2') 1/2
									.range-small.col-1
										a(href='https://www.brightonholidayhomes.co.uk/sleeps/range/1-2') 3/5
									.range-small.col-1
										a(href='https://www.brightonholidayhomes.co.uk/sleeps/range/1-2') 6/9
									.range-large.col-1
										a(href='https://www.brightonholidayhomes.co.uk/sleeps/range/1-2') 10/19
									.range-large.col-1
										a(href='https://www.brightonholidayhomes.co.uk/sleeps/range/1-2') 20/30
									.range-large.col-1
										a(href='https://www.brightonholidayhomes.co.uk/sleeps/range/1-2') 31/40
						// LANDLORD 3D REVOLUTION
						.col-3.d-flex.flex-row-reverse.pb-10px
							a(href='www.brightonholidayhomes.co.uk')
